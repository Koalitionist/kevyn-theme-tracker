{%- comment -%} Auto Generated Variables start {%- endcomment -%}
{%- liquid
  assign sticky = section.settings.sticky
  assign breadcrumbs__show = section.settings.breadcrumbs__show
  assign gallery__layout = section.settings.gallery__layout
  assign gallery__show_thumbnails = section.settings.gallery__show_thumbnails
  assign gallery__size = section.settings.gallery__size
  assign gallery__loop_videos = section.settings.gallery__loop_videos
  assign section_layout = section.settings.section_layout
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign color_scheme = section.settings.color_scheme
-%}
{%- comment -%} Auto Generated Variables end {%- endcomment -%}

{% liquid
  assign product_form_id = 'product-form-' | append: section.id | append: "-" | append: product.id
  assign variant = product.selected_or_first_available_variant

  case section.settings.gallery__size
    when 'small'
      assign desktopGridClasses = "lg:grid-cols-[1fr_650px]"
    when 'medium'
      assign desktopGridClasses = "lg:grid-cols-[1fr_450px]"
    when 'large'
      assign desktopGridClasses = "lg:grid-cols-[1fr_350px]"
  endcase

  if section.settings.sticky
    assign stickyClasses = 'lg:pb-24 lg:sticky lg:top-spacing-8 lg:max-h-[calc(100vh-var(--top-bar-spacing))] lg:overflow-y-auto scrollbar-none -mx-2 px-2 transition-all ease-linear'
    assign stickyAttributes = 'data-product-sticky-content'
  endif
%}
  
<div class="{{ section_layout }} {{ color_scheme }} {{ padding_top }} {{ padding_bottom }} {{ responsive_visibility }} relative">
  <div class="absolute inset-0 z-10 transition-all duration-75 ease-linear bg-theme-bg"
       data-product-loading-screen></div>
  <article class="w-full grid gap-8 {{ desktopGridClasses }}"
           data-product-handle="{{ product.handle }}"
           data-product-form-id="{{ product_form_id }}"
           data-product-selected-variant="{{ product.selected_or_first_available_variant.id }}">
    <script type="application/json"
            data-section-settings>{{- section.settings | json -}}</script>
    <section data-product-gallery>
      {% render "product.gallery" %}
    </section>
    <section class="h-min grid gap-2 {{ stickyClasses }}" {{ stickyAttributes }}>
      {% if breadcrumbs__show %}
        {% render '_breadcrumbs' %}
      {% endif %}

      <div class="grid grid-cols-12 gap-3"
           data-product-blocks>
        <div data-product-block="side_effects"
             class="hidden"></div>
        {% for block in section.blocks %}
          {% case block.type %}
            {% when '@app' %}
              <div data-product-block="@app"
                   class="col-span-12" {{ block.shopify_attributes }}>
                {% render block %}
              </div>
            {% when 'text' %}
              {% render 'product.text', block: block %}
            {% when 'rating' %}
              {% render 'product.rating', block: block, reviews: product.metafields.reviews %}
            {% when 'image' %}
              {% render 'product.image', block: block %}
            {% when 'title' %}
              {% render 'product.title', block: block %}
            {% when 'vendor' %}
              {% render 'product.vendor', block: block %}
            {% when 'sku' %}
              {% render 'product.sku', block: block, variant: variant %}
            {% when 'price' %}
              {% render 'product.price', block: block, variant: variant %}
            {% when 'variant_selector' %}
              {% render 'product.variant_selector', block: block, variant: variant %}
            {% when 'product_sibling' %}
              {% render 'product.product_sibling', block: block %}
            {% when 'quantity_selector' %}
              {% render 'product.quantity_selector', block: block %}
            {% when 'custom_liquid' %}
              {% render 'product.custom_liquid', block: block %}
            {% when 'description' %}
              {% render 'product.description', block: block %}
              {% comment %} CUSTOM SECTION BLOCK ---- START {% endcomment %}
            {% when 'description_tabs' %}
              {% render 'custom-product-andrews.description_tabs', block: block %}
              {% comment %} CUSTOM SECTION BLOCK ---- END {% endcomment %}
            {% when 'collapsible_tab' %}
              {% render 'product.collapsible_tab', block: block %}
            {% when 'share' %}
              {% render 'product.share', block: block %}
            {% when 'dynamic_buy_buttons' %}
              {% render 'product.dynamic_buy_buttons', block: block, variant: variant %}
            {% when 'buy_buttons' %}
              {% render 'product.buy_buttons', block: block, variant: variant %}
            {% when 'complementary' %}
              {% render 'product.complementary', block: block %}
            {% when 'icon_with_text' %}
              {% render 'product.icon_with_text', block: block %}
            {% when 'inventory' %}
              {% render 'product.inventory', block: block, variant: variant %}
            {% when 'pre_order' %}
              {% render 'product.pre_order', block: block, variant: variant %}
          {% endcase %}
        {% endfor %}
      </div>
    </section>

    {% render '_seo', type: 'product' %}
  </article>
</div>
{% render '_color-group-access', colorGroup: section.settings.color_scheme, paddingTop: section.settings.padding_top, paddingBottom: section.settings.padding_bottom %}
