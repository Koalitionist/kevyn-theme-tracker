        "314c7709-5263-46c5-95c5-73bee2ae5af0": {
          "type": "text",
          "settings": {
            "title": "<h1>THE FUNDAMENTALS<\/h1>",
            "title_font": 3,
            "opacity": 100,
            "alignment_desktop": "[text-align:inherit]",
            "alignment_mobile": "max-md:[text-align:inherit]",
            "margin_top": 0,
